<div class = "post-main-col">


<%= simple_form_for(@post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :photo %>


  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

</div>